{
  "_version": "1.7.0",
  "sap.app": {
    "id": "my.cave.overview",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "tags": {
      "keywords": []
    },
    "ach": "ACH",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "/api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "offline": false
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_belize", "sap_fiori_3_dark"]
  },
  "sap.ui5": {
    "resources": {
      "js": [],
      "css": []
    },
    "dependencies": {
      "minUI5Version": "1.65.0",
      "libs": {},
      "components": {}
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "mainService": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "extends": {
      "extensions": {}
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "analytical"
  },
  "sap.ovp": {
    "globalFilterModel": "mainService",
    "globalFilterEntityType": "",
    "containerLayout": "resizable",
    "enableLiveFilter": false,
    "considerAnalyticalParameters": false,
    "cards": {
      "card00": {
        "model": "mainService",
        "template": "sap.ovp.cards.v4.list",
        "settings": {
          "title": "Représentativité des cépages dans ma cave",
          "sortBy": "name",
          "sortOrder": "descending",
          "listFlavor": "bar",
          "annotationPath": "com.sap.vocabularies.UI.v1.LineItem",
          "category": "test",
          "entitySet": "VinPerCepage"
        }
      },
      "card01": {
        "model": "mainService",
        "template": "sap.ovp.cards.v4.list",
        "settings": {
          "title": "Type de vin dans ma cave",
          "sortBy": "color",
          "sortOrder": "descending",
          "listFlavor": "bar",
          "annotationPath": "com.sap.vocabularies.UI.v1.LineItem",
          "category": "test",
          "entitySet": "OverviewVinColor"
        }
      },
      "card02": {
        "model": "mainService",
        "template": "sap.ovp.cards.v4.charts.analytical",
        "chartType": "cardchartsdonut",
        "settings": {
          "title": "Type de vin dans ma cave",
          "entitySet": "OverviewVinColor",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart"
        }
      },
      "card03": {
        "model": "mainService",
        "template": "sap.ovp.cards.v4.charts.analytical",
        "chartType": "cardchartscolumnstacked",
        "settings": {
          "title": "Type de vin dans ma cave",
          "entitySet": "VinAnalytics",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart"
        }
      },
      "card04": {
        "model": "mainService",
        "template": "sap.ovp.cards.v4.charts.analytical",
        "chartType": "cardchartline",
        "settings": {
          "title": "Evolution des superficies de cépages",
          "entitySet": "SuperficieEvolution",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart"
        }
      }
    }
  }
}
